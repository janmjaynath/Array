public class binarysearch{
  public static int Binarysearch(int n[] , int key){

 int start = 0;
 int end = n.length-1;
 while(start <= end){
    int mid = (start + end)/2;
    if(n[mid] == key){
        return mid;
    }    
    if(key > n[mid]){
        start = mid + 1;
    }
    else{
        start = mid -1;
    }
 }
    return -1;
 }
 public static void main(String args []){
    int n[] = {1,2,3,4,5,6};
    int key = 5;
    System.out.println(Binarysearch(n,key));
 }

 }
